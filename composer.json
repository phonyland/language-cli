{
    "name": "phonyland/language-cli",
    "description": "\uD83D\uDCBB Language CLI for \uD83D\uDE43 Phony Framework",
    "homepage": "http://phony.land",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/phonyland/language-cli/issues",
        "source": "https://github.com/phonyland/language-cli"
    },
    "authors": [
        {
            "name": "Yunus Emre Delig√∂z",
            "email": "ye@deligoz.me"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "laravel-zero/framework": "^8.8",
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/phony-language-cli"]
}
